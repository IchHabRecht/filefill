{
    "name": "ichhabrecht/filefill",
    "description": "Find and fetch missing local files from different remotes",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3",
        "extension",
        "file",
        "missing",
        "placeholder"
    ],
    "homepage": "https://github.com/IchHabRecht/filefill",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Nicole Hummel",
            "email": "nicole-typo3@nimut.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "typo3/cms-core": "^13.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5 || ^11.3",
        "php-parallel-lint/php-parallel-lint": "^1.4",
        "typo3/testing-framework": "^8.2.3 || ^9.1.2"
    },
    "autoload": {
        "psr-4": {
            "IchHabRecht\\Filefill\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IchHabRecht\\Filefill\\Tests\\": "Tests/"
        }
    },
    "replace": {
        "typo3-ter/filefill": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.x-dev"
        },
        "typo3/cms": {
            "extension-key": "filefill",
            "app-dir": ".Build",
            "web-dir": ".Build/public"
        }
    }
}
